%%% Hàm kiểm tra tham số là số hay là command
\newcommand{\IfIsCommand}[3]{%
    \checkArg@aux#1\@nil{#2}{#3}%
}
% Hàm phụ trợ tách token đầu tiên
\def\checkArg@aux#1#2\@nil{%
    \ifcat\noexpand#1\relax%
        \expandafter\@firstoftwo%
    \else%
        \expandafter\@secondoftwo%
    \fi%
}
%%% Tạo lệnh đổi khung cho theorem
\newcommand{\setTheoBox}{\@ifstar{\@setTheoBoxStar}{\@setTheoBoxNoStar}}
%%% Tạo lệnh đổi khung cho theorem nếu có đánh số
\newcommand{\@setTheoBoxNoStar}[3]{
	% Cài đặt màu khung và nền của tcolorbox theo tên môi trường
	\@ifundefined{sb@#1colframe}{\setboxColframe{#1}{\sb@defaultColframe}}{}
	\@ifundefined{sb@#1colback}{\setboxColback{#1}{\sb@defaultColback}}{}
	\@ifundefined{sb@#1colbacktitle}{\setboxColbacktitle{#1}{\sb@defaultColbacktitle}}{}
	% Bật hiển thị khung khi dùng trong tcolorbox khác theo mặc định
	\showboxInTcb{#1}
	% Khai báo lại môi trường
	\RenewEnviron{#1}[1][]{
		% Kiểm tra xem có bỏ qua không
		\checkSkipFlag{#1}
		% Tăng counter của môi trường #1 một đơn vị
		\edef\tmpctr{\getcountername{#1}}
		\stepcounter{\tmpctr}
		% Tạo biến tạm thời để ẩn/hiện khung
		\csname Ifsb@#1hideboxInTcb\endcsname
			\sb@hideboxInTcbtrue
		\else
			\sb@hideboxInTcbfalse
		\fi
		% Tiến hành xử lý
		\Ifsb@skipflag
		\else
			% Khai báo màu
			\def\sb@colbacktitle{\csname sb@#1colbacktitle\endcsname}
			\def\sb@colback{\csname sb@#1colback\endcsname}
			\def\sb@colframe{\csname sb@#1colframe\endcsname}
			% Khai báo số thứ tự
			\def\sb@Currentlabel{\csname the#1\endcsname}
			% Khai báo tiêu đề
			\def\sb@labelthm{#3}
			% Khai báo tiêu đề PHỤ
			\def\sb@sublabelthm{##1}
			% Khai báo tiêu đề PHỤ với ngoặc
			\ifthenelse{\equal{##1}{}}{\def\sb@sublabelthmBracket{}}{\def\sb@sublabelthmBracket{(##1)}}
			% Khai báo tiêu đề RÚT GỌN, không có tiêu đề phụ
			% Ví dụ "Định nghĩa 1"
			\def\sb@labelthmshort{\sb@labelthm~\sb@Currentlabel}
			% Khai báo tiêu đề RÚT GỌN, có dấu "." ở cuối
			% Ví dụ "Định nghĩa 1."
			\def\sb@labelthmshortDot{\sb@labelthm~\sb@Currentlabel.}
			% Khai báo tiêu đề ĐẦY ĐỦ, có tiêu đề phụ
			% Ví dụ "Định nghĩa 1 (Tên định nghĩa)"
			\ifthenelse{\equal{##1}{}}{\def\sb@labelthmfull{\sb@labelthm~\sb@Currentlabel}}{\def\sb@labelthmfull{\sb@labelthm~\sb@Currentlabel~(\sb@sublabelthm)}}
			% Khai báo tiêu đề ĐẦY ĐỦ, có tiêu đề phụ, có dấu "." ở cuối
			% Ví dụ "Định nghĩa 1 (Tên định nghĩa)"
			\ifthenelse{\equal{##1}{}}{\def\sb@labelthmfullDot{\sb@labelthm~\sb@Currentlabel.}}{\def\sb@labelthmfullDot{\sb@labelthm~\sb@Currentlabel~(\sb@sublabelthm).}}
			% Áp dụng khung
			\IfIsCommand{#2}{#2}{\sb@applyTheoBox{#2}}
			% Tăng cấp box
			\addtocounter{sb@boxLevel}{1}
			% Lưu \endbox theo cấp box
			\expandafter\edef\csname sb@currBoxLevel\endcsname{\roman{sb@boxLevel}}
			\expandafter\let\csname endbox\sb@currBoxLevel\endcsname\endbox
			% Bắt đầu tcolorbox
			\sb@beginbox
			% Xoá labelthm nếu có \immini ngay đầu \BODY
			\expandafter\sb@addTitle%
			% Nội dung
			\BODY
			% Kết thúc tcolorbox nếu còn trong tcolorbox
			% Nếu môi trường ex đã đóng lại tcolorbox trước khi đến chc thì sẽ bỏ qua
			\ifsb@InTcolorbox\endbox\fi%
			% Giảm cấp box
			\addtocounter{sb@boxLevel}{-1}
			% Hoàn trả \endbox theo cấp box khi vừa dùng \loigiai
			\ifsb@JustSolution
				\sb@JustSolutionfalse
				\expandafter\edef\csname sb@currBoxLevel\endcsname{\roman{sb@boxLevel}}
				\gdef\endbox{\csname endbox\sb@currBoxLevel\endcsname}
			\fi
		\fi
	}
	% Chạy lệnh fix nếu đã khai báo ex_test
	\@ifpackageloaded{ex_test}
	{
		\ifthenelse{\equal{#1}{ex}}
		{
			\setTheoBox{chc}{#2}{#3}
		}{}
		\fixshowans{#1}
	}
	{}
}
